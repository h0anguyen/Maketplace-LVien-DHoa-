    "browserify": "17.0.0",
    "child_process": "1.0.2",
    "cookie-parser": "1.4.6",
    "dayjs": "^1.11.12",
    "decimal.js": "^10.4.3",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-flash": "0.0.2",
    "express-session": "1.17.1",
    "faker": "^6.6.6",
    "font-awesome": "4.7.0",
